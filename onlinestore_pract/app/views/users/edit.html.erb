<h1>Edit Article</h1>
<%= flash[:error]%>

<%= form_for @user do |f| %>
  <%= f.label :login%>
  <%= f.text_field :name%> <br />

  <%= f.label :email%>
  <%= f.text_field :email%> <br />

  <%= f.label :name%>
  <%= f.text_field :name%> <br />

  <%= f.label :address%>
  <%= f.text_field :address%> <br />

  <%= f.label :phone_number%>
  <%= f.text_field :phone_number%> <br />

  <%= f.label :birthday%>
  <%= f.text_field :birthday%> <br />

  <%= f.label :gender%>
  <%= f.text_field :gender%> <br />

  <%= f.submit "Save"%>
<%end%>
